{
  "name": "comunica-sparql-expeval",
  "version": "0.0.1",
  "description": "Promise based SPARQL Expression Evaluator",
  "main": "dist/src/lib.js",
  "scripts": {
    "test": "jest",
    "test-debug-lin": "node --nolazy --debug-brk=5858 ./node_modules/.bin/jest",
    "test-debug-win": "node --debug-brk=5858 ./node_modules/jest/bin/jest.js",
    "prepare": "npm run build",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wschella/Comunica-SPARQL-ExpEval.git"
  },
  "keywords": [
    "sparql",
    "rdf",
    "expression-engine",
    "promise"
  ],
  "author": "Wout Schellaert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wschella/Comunica-SPARQL-ExpEval/issues"
  },
  "homepage": "https://github.com/wschella/Comunica-SPARQL-ExpEval#readme",
  "devDependencies": {
    "@types/asynciterator": "^1.1.0",
    "@types/benchmark": "^1.0.30",
    "@types/bluebird": "^3.5.18",
    "@types/jest": "^21.1.4",
    "@types/lodash": "^4.14.105",
    "@types/lodash.isequal": "^4.5.2",
    "@types/node": "^8.0.34",
    "@types/rdf-data-model": "^1.0.1",
    "benchmark": "^2.1.4",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-typescript": "^3.2.2",
    "jest": "^22.4.0",
    "jest-html-reporter": "^0.5.3",
    "merge-stream": "^1.0.1",
    "ts-jest": "^21.1.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.6.0"
  },
  "dependencies": {
    "asynciterator": "^1.1.0",
    "bluebird": "^3.5.1",
    "immutable": "^3.8.2",
    "lodash.isequal": "^4.5.0",
    "rdf-data-model": "^1.0.0",
    "rdf-js": "^1.0.1",
    "sparqlalgebrajs": "^0.7.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/archive/",
      "/dist/",
      "/test/util/"
    ],
    "testResultsProcessor": "./node_modules/jest-html-reporter",
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  }
}
