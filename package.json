{
  "name": "sparqlee",
  "version": "0.0.1-rc7",
  "description": "A simple SPARQL expression evaluator library",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test-debug-lin": "node --nolazy --debug-brk=5858 ./node_modules/.bin/jest",
    "test-debug-win": "node --debug-brk=5858 ./node_modules/jest/bin/jest.js",
    "prepare": "npm run build",
    "watch": "gulp watch",
    "build": "gulp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wschella/Sparqlee.git"
  },
  "keywords": [
    "sparql",
    "expression",
    "rdf",
    "evaluator"
  ],
  "author": "Wout Schellaert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wschella/Sparqlee/issues"
  },
  "homepage": "https://github.com/wschella/Sparqlee#readme",
  "devDependencies": {
    "@types/asynciterator": "^1.1.0",
    "@types/benchmark": "^1.0.30",
    "@types/bluebird": "^3.5.18",
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.105",
    "@types/lodash.isequal": "^4.5.2",
    "@types/node": "^10.11.0",
    "@types/rdf-data-model": "^1.0.1",
    "asynciterator": "^2.0.0",
    "benchmark": "^2.1.4",
    "coveralls": "^3.0.2",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-typescript": "^5.0.0-alpha.3",
    "jest": "^23.6.0",
    "jest-html-reporter": "^2.4.2",
    "merge-stream": "^1.0.1",
    "ts-jest": "^23.10.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "immutable": "^3.8.2",
    "rdf-data-model": "^1.0.0",
    "rdf-js": "^1.0.1",
    "rdf-string": "^1.1.1",
    "sparqlalgebrajs": "^1.1.0"
  },
  "jest": {
    "testEnvironment": "node",
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/archive/",
      "/dist/",
      "/test/util/"
    ],
    "testResultsProcessor": "./node_modules/jest-html-reporter",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  }
}
